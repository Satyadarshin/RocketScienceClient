n  },\n  props: {\n    spotlightOn: String\n  },\n  components: {\n    appAuthorTable: authorTable,\n    appAuthorSpotlight: authorSpotlight\n  },\n  mounted() {\n    sfDataService.fetchTextRecords('authors')\n    .then((jsonData) => {\n        this.authors = jsonData.results\n    })\n    .catch((error) => {\n        console.error('Error:', error);\n    })\n  }\n}\n</script>\n"]}]}