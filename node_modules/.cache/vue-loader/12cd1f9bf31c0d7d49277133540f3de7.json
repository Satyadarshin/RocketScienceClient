olumns\">\n                    <a href=\"#\" @click.prevent=\"sortBy('column')\">{{ column }}</a>\n                </th>\n            </tr>\n        </thead>\n        <tbody>\n            <tr \n            v-for=\"(award, index) in selectedAward\"\n            :key=\"index\"\n            itemscope itemtype=\"http://schema.org/Book\"\n            >\n                <td>{{ award.year }}</td>\n                <td itemprop=\"author\">{{ award.winner.author }}</td>\n                <td itemprop=\"name\">{{ award.winner.title }}</td>\n            </tr>\n        </tbody>\n    </table>\n</div>\n</template>\n\n<script>\nexport default {\n    data: () => {\n        return {\n            sortKey: 'year',\n            sortDirection: 'asc',\n            columns: [] //create an array to contain the supplied array's keys to use as column headers\n        }\n    },\n    props: {\n        who: String,\n        what: String,\n        selectedAward: Array\n    },\n    methods: {\n        sortBy: (sortkey) => {\n        }\n    },\n    computed: {\n        createHeaders() {\n            let test = this.selectedAward[0]\n            return this.columns = Object.keys(test)\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\ntable {\n    margin: 1rem 0;\n    width: 100%;\n    border-collapse: collapse;\n    caption {\n        padding: 1rem 0;\n        background-color: $table-caption;\n        color:$white;\n        span {\n            text-transform: capitalize;\n        }\n    } \n    tr:nth-child(even) {\n        background-color: $lightgrey;\n    }\n    td, th { \n       // text-align: left;\n        padding: .5rem  0 .5rem .25rem;\n    }\n    td {\n        &:nth-child(2) { \n            text-align: right \n        }   \n        &:last-child { \n            text-align: left; padding-left: 1rem;   \n        }\n    }\n    thead {\n        border-bottom: 1px solid $table-border;\n        th {\n            a {\n                text-decoration: none;\n                color: $oil;\n                text-transform: capitalize;\n            }\n            cursor: pointer\n        }\n    }\n}\n</style>"]}]}